{
  "expo": {
    "name": "SmartBag",
    "slug": "smartbag",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "smartbag",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "EXPO_PUBLIC_API_URL": "http://10.0.0.88:8000/api/",
      "EXPO_PUBLIC_API_URL_PROD": "https://smartbag-backend-oqlt.onrender.com",
      "EXPO_PUBLIC_OLA_KRUTRIM_API_KEY": "dzfpTC3yjmbi4eQLsDzBwBdGyYYdZXN80BFR73kQ",
      "eas": {
        "projectId": "37775160-3c66-48ef-988a-82efae359109"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ios.smartbag",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to help you select your delivery address.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to help you select your delivery address.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "10.0.0.88": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            },
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            },
            "smartbag.onrender.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "package": "com.ios.smartbag"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to help you select your delivery address."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1029983278193-mgl5t7mtqajk2a3vocs6qchbir317o9q"
        }
      ],
      "expo-router"
    ],
    "owner": "nitin3150"
  }
}